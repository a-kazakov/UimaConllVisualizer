<taeDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <frameworkImplementation>org.apache.uima.cpp</frameworkImplementation>
    <primitive>true</primitive>
    <annotatorImplementationName>Visualizer</annotatorImplementationName>

    <analysisEngineMetaData>

        <name>Visualizer</name>
        <description>Creates a visualization of CONLL-U annotations</description>
        <version>1.0</version>
        <vendor>Part</vendor>

        <configurationParameters>
            <configurationParameter>
                <name>FileName</name>
                <description>Output file name</description>
                <type>String</type>
                <multiValued>false</multiValued>
                <mandatory>true</mandatory>
            </configurationParameter>
        </configurationParameters>
        <configurationParameterSettings>
          <nameValuePair>
            <name>FileName</name>
            <value>
              <string>output.html</string>
            </value>
          </nameValuePair>
        </configurationParameterSettings>

        <typeSystemDescription>
            <types>
                <typeDescription>
                    <name>net.akazakov.KeyValue</name>
                    <description></description>
                    <supertypeName>uima.cas.TOP</supertypeName>
                    <features>
                        <featureDescription>
                            <name>Key</name>
                            <description></description>
                            <rangeTypeName>uima.cas.String</rangeTypeName>
                        </featureDescription>
                        <featureDescription>
                            <name>Value</name>
                            <description></description>
                            <rangeTypeName>uima.cas.String</rangeTypeName>
                        </featureDescription>
                    </features>
                </typeDescription>
                <typeDescription>
                    <name>net.akazakov.ConllAnnotation</name>
                    <description></description>
                    <supertypeName>uima.tcas.Annotation</supertypeName>
                    <features>
                        <featureDescription>
                            <name>TopLevel</name>
                            <description></description>
                            <rangeTypeName>uima.cas.Boolean</rangeTypeName>
                        </featureDescription>
                        <featureDescription>
                            <name>Form</name>
                            <description></description>
                            <rangeTypeName>uima.cas.String</rangeTypeName>
                        </featureDescription>
                        <featureDescription>
                            <name>Lemma</name>
                            <description></description>
                            <rangeTypeName>uima.cas.String</rangeTypeName>
                        </featureDescription>
                        <featureDescription>
                            <name>UPosTag</name>
                            <description></description>
                            <rangeTypeName>uima.cas.String</rangeTypeName>
                        </featureDescription>
                        <featureDescription>
                            <name>XPosTag</name>
                            <description></description>
                            <rangeTypeName>uima.cas.String</rangeTypeName>
                        </featureDescription>
                        <featureDescription>
                            <name>Feats</name>
                            <description></description>
                            <rangeTypeName>uima.cas.FSArray</rangeTypeName>
                            <elementType>net.akazakov.KeyValue</elementType>
                        </featureDescription>
                        <featureDescription>
                            <name>Misc</name>
                            <description></description>
                            <rangeTypeName>uima.cas.FSArray</rangeTypeName>
                            <elementType>net.akazakov.KeyValue</elementType>
                        </featureDescription>
                        <featureDescription>
                            <name>Children</name>
                            <description></description>
                            <rangeTypeName>uima.cas.FSArray</rangeTypeName>
                            <elementType>net.akazakov.ConllAnnotation</elementType>
                        </featureDescription>
                    </features>
                </typeDescription>
            </types>
        </typeSystemDescription>

        <capabilities>
            <capability>
                <inputs>
                    <type>org.ConllAnnotation</type>
                </inputs>
                <outputs>
                </outputs>
            </capability>
        </capabilities>

    </analysisEngineMetaData>
</taeDescription>
